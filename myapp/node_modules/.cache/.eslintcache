ages/Login.js",["65","66"],[],"import React, {useState, useEffect} from 'react';\nimport '../css/Login.css';\nimport logo from '../logo.png';\nimport '../fonts/Bayon-Regular.ttf';\nimport {Link} from 'react-router-dom';\n\nfunction Login() {\n\n    const [email, setEmail] = useState('');\n    const [password, setPassword] = useState('');\n    const [message, setMessage] = useState('');\n\n    const handleSubmit = (event) => {\n        event.preventDefault();\n        fetch('http://localhost:3000/login', {\n            method: 'POST',\n            headers: {\n                'Content-Type': 'application/json',\n            },\n            body: JSON.stringify({email, password}),\n        })\n            .then((response) => {\n                if (response.ok) {\n                    setMessage('Login successful');\n                    return response.json();\n                } else if (response.status === 401) {\n                    setMessage('Incorrect email or password');\n                } else {\n                    throw new Error('An error occurred while authenticating');\n                }\n            })\n            .then(data => {\n                console.log(data);\n                if (data.message === \"waiter\") {\n                    window.location.href = '/waiter';\n                }\n                if (data.message === \"kitchen\") {\n                    window.location.href = '/kitchen';\n                }\n                if (data.message === \"customer\") {\n                    window.location.href = '/menuL';\n                }\n            })\n            .catch((error) => {\n                console.error(error);\n                setMessage('An error occurred while authenticating');\n            });\n    };\n\n    return (\n        <div className=\"login-form\">\n            <div className=\"Login\">\n                <header className=\"App-header\">\n                    <Link to=\"/\">\n                        <img src={logo} alt=\"the logo\" className=\"header-image\"/>\n                    </Link>\n                </header>\n                <div className=\"form-group\">\n                    <div className=\"label-wrapper\">\n                        <label className=\"email-label\">Email:</label>\n                    </div>\n                    <div className=\"input-wrapper\">\n                        <input type=\"email\" value={email} onChange={(e) => setEmail(e.target.value)}/>\n                    </div>\n                </div>\n\n                <div className=\"form-group\">\n                    <div className=\"label-wrapper\">\n                        <label className=\"password-label\">Password:</label>\n                    </div>\n                    <div className=\"input-wrapper\">\n                        <input type=\"password\" value={password} onChange={(e) => setPassword(e.target.value)}/>\n                    </div>\n                </div>\n                <div className=\"form-group\">\n                    <div className=\"buttons-lgrg\">\n                        <button className=\"login-Button\" onClick={handleSubmit}>\n                            Login\n                        </button>\n                        <Link to=\"../register\">\n                            <button className=\"register-button\"> REGISTER</button>\n                        </Link>\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Login","/Users/matty/project/PROJECT/myapp/src/pages/Waiter.js",[],[],"/Users/matty/project/PROJECT/myapp/src/pages/Register.js",["67"],[],"/Users/matty/project/PROJECT/myapp/src/pages/MenuLoggedIn.js",["68"],[],"/Users/matty/project/PROJECT/myapp/src/pages/Cart.js",["69"],[],"/Users/matty/project/PROJECT/myapp/src/pages/EditMenu.js",[],[],"/Users/matty/project/PROJECT/myapp/src/pages/User.js",["70","71"],[],{"ruleId":"72","severity":1,"message":"73","line":177,"column":25,"nodeType":"74","endLine":177,"endColumn":48}